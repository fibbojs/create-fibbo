  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd playground && fibbo dev",
    "build": "tsdown"
  },
  "dependencies": {
    "@fibbojs/3d": "^0.0.20",
    "@fibbojs/devtools": "^0.0.20",
    "@fibbojs/event": "^0.0.20",
    "fibbo": "^0.0.20"
  },
  "devDependencies": {
    "tsdown": "^0.12.9"
  }
}
